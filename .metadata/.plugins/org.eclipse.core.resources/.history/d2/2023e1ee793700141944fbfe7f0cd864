package test;

public class Duplicate_sequence

{
   
	private String input;
	
	Duplicate_sequence(String s)
	{
		
		this.input=s;
		
		
	}
	
	
	public String remove_Duplicate()
	{
		String modified=new String();
		for(int i=0;i<this.input.length()-1;i++)
		{
			
	       char  x=input.charAt(i);
	       modified+=""+x;
	       char y=input.charAt(i+1);
	       if(x!=y)
	       {
	    	   modified+=""+y;
	       }
		
			System.out.println(modified);
	        
		}
		
	
		
		return modified;
	}
	
	
	
	
}
