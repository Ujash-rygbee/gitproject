package lab3;
import java.util.Scanner;

public class insertion_sort 
{
  private int[] array=new int[5];
  Scanner scan=new Scanner(System.in); 
  
  
  
  public void sort()
  {
	  int k=0;
	  int l=0;
	  
	for(int i=0;i<5;i++)
	{
		int j=0;
		array[i]=scan.nextInt();
		System.out.println(array[i]);
		
		while(array[j]<array[i])
		{
		  j++;	
			
		}
		
		k=array[j+1];
		array[j+1]=array[i];
	     j=j+2;
	
			
	
		

		
		
	}
	
	for(int i=0;i<5;i++)
	{
		System.out.println(array[i]);
	}
	
	  
	  
	  
  }
  
  
	
	
}
