package exam;

public class circular
 { 
  public static void main(String[] args)
  {
	int a[]={23,34,54,87,7,10,13,16,19};
    binary(a,0,10,a.length/2);
  
  
  }
	
  
    public static int binary(int b[],int m,int key,int n)
    {
    	int i=0;
    	if(b[n]==key)
    	i=1;
    
    	else if(b[m]<b[n])
    	{
    	  	if(b[m]<key && key<b[n])
    	  	{
    	  		
    	  		n=n/2;
    	  		binary(b,m,n);
    	  		
    	  	}
    	}
    
    
    
    
    
    return i;
    }
  
  
	  
	
	
  }
