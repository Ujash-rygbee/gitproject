package test;

public class Duplicate_sequence

{
   
	private String input;
	
	Duplicate_sequence(String s)
	{
		
		this.input=s;
		
		
	}
	
	
	public String remove_Duplicate()
	{
		String modified=new String();
		for(int i=0;i<=this.input.length()-1;i++)
		{
			
			char character=input.charAt(i);
			char another=input.charAt(i+1);
	        if(character!=another)
	        {
	        	modified+=""+character;
	        	if(i==input.length()-1)
	        	{
	        		modified+=""+another;
	        	}
	        	
	        	
	        }
	        
	        
			
			
	        System.out.println(i);
		}
		
		
		
		return modified;
	}
	
	
	
	
}
